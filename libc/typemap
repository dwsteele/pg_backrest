INPUT
T_SV
    STRLEN ${var}SvSize;
    void *${var}SvPtr = SvPV($arg, ${var}SvSize);
    (void)${var}SvPtr;
    (void)${var};
    $var = $arg;

INPUT
T_PV
    STRLEN ${var}PvSize;
    $var = (void *)SvPV($arg, ${var}PvSize);
    ((char *)$var)[${var}PvSize] = 0;
